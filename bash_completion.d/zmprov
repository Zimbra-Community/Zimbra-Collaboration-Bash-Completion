# arguments:
#   1 = current word to be completed
_zmprov_accountList()
{
    COMPREPLY=( "${COMPREPLY[@]}"
        $( compgen -W "$( ${ldapsearch:-ldapsearch} -x -H $ldap_master_url -D $zimbra_ldap_userdn -w $zimbra_ldap_password '(objectClass=zimbraAccount)' mail | egrep '^mail' | awk '{print $2}' | tr '\n' ' ')" -- "$1" ) )
}

