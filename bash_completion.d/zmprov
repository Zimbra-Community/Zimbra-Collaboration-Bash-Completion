# arguments:
#   1 = current word to be completed
_zmprov_domainList()
{
    COMPREPLY=( "${COMPREPLY[@]}"
        $( compgen -W "$( ${ldapsearch:-ldapsearch} -x -H $ldap_master_url -D $zimbra_ldap_userdn -w $zimbra_ldap_password '(objectClass=zimbraDomain)' zimbraDomainName | egrep '^zimbraDomainName' | awk '{print $2}' | tr '\n' ' ')" -- "$1" ) )
}

# arguments:
#   1 = current word to be completed
_zmprov_calendarList()
{
    COMPREPLY=( "${COMPREPLY[@]}"
        $( compgen -W "$( ${zmprov:-zmprov} getAllCalendarResources)" -- "$1" ) )
}

# arguments:
#   1 = current word to be completed
_zmprov_serverList()
{
    COMPREPLY=( "${COMPREPLY[@]}"
        $( compgen -W "$( ${ldapsearch:-ldapsearch} -x -H $ldap_master_url -D $zimbra_ldap_userdn -w $zimbra_ldap_password '(objectClass=zimbraServer)' cn |  egrep '^cn' |  awk '{print $2}' | tr '\n' ' ')" -- "$1" ) )
}

